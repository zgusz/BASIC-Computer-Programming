#include <stdio.h>

int main() {
    int dayCode, hour;
    
    if (scanf("%d %d",&dayCode, &hour) != 2) {
        return 1;
    }
    
    switch (dayCode) {
        case 1:
        case 2:
        case 3:
        case 4:
        case 5:
            if (8 <= hour && hour <= 17) {
                printf("System Runnning (Workday)");
            } else {
                printf("System Idle (Off-hours)");
            }
            break;
        case 6:
        case 7:
            printf("Weekend Relax Mode");
            break;
        default:
            printf("Invalid Day Code");
            break;
    }
    
    return 0;
}
