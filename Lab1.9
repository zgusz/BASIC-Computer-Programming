#include <stdio.h>

int main() {
    int zoneCode;
    float weight_kg;
    float totalCost = 0.0;
    
    if (scanf("%d %f", &zoneCode, &weight_kg) != 2) {
        return 1;
    }
    
    switch (zoneCode) {
        case 1:
            if (weight_kg <= 5) {
                totalCost = 50.00;
            } else {
                totalCost = 80.00;
            }
            break;
            
        case 2:
            if (weight_kg <= 10) {
                totalCost = 150.00;
            } else {
                totalCost = 250.00;
            }
            break;
            
        case 3:
            totalCost = 500.00;
            break;
            
        default:
            printf("Invalid Zone Code\n");
            break;
    }
    
    if (totalCost >0.0 || zoneCode > 3 || zoneCode < 1) {
        printf("%.2f\n", totalCost);
    }
    
    return 0;
}
